package herencia1;

import java.util.Date;

public class ProdRefrig extends Producto {
	
	String tiempoRegrig;

	public ProdRefrig(Date caducidad, String nLote, String nombreProd, String tiempoRegrig) {
		super(caducidad, nLote, nombreProd);
		this.tiempoRegrig=tiempoRegrig;
	}
	
	public void imprimirProd() {
		System.out.println("Producto " +super.nombreProd+ " del lote " +super.getnLote()+
				"\n y tiempo refrigeraso " +tiempoRegrig);
	}
	

}
