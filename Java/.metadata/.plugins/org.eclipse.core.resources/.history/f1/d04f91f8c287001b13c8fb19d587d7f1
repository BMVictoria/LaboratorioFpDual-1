package herencia1;

import java.util.Date;

public class ProdFresc extends Producto {
	
	String tiempoFresc;

	public ProdFresc(Date caducidad, String nLote, String nombreProd, String tempRec) {
		super(caducidad, nLote, nombreProd);
		this.tiempoFresc=tempRec;
	}
	
	public void imprimirProd() {
		System.out.println("Producto " +super.nombreProd+ " del lote " +super.getnLote()+
				"\n y tiempo fresco " +tiempoFresc);
	}
	

}
