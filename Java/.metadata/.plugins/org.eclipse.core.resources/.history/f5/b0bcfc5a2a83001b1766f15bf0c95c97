package logica;

import java.util.Scanner;

public class Eje2 {


	
	public static void main(String[] args){
		
		Scanner sc = new Scanner(System.in);
		String alumno = null;

		System.out.println("Escribe el nombre del alumno");
		
		try {
			
			alumno = sc.nextLine();
			
		} catch (Exception e) {
		}
		

		System.out.println("Alumno: " +alumno);
		
		sc.close();
	}
}
